<script type="text/javascript">
    function AddCardItemCount() {
        var cartItems = [];
        cartItems = JSON.parse(localStorage.getItem("userCarts"));
        if (cartItems != null) {
            $("#PRODUCTCOUNT").text(cartItems.length);
        }
    };

    function addToCart(productId) {
        $.ajax({
            type: 'GET',
            url: '/Home/CartJsonRecord?productId=' + productId,
            contentType: 'application/json; charset=utf-8',
            dataType: 'json',
            async: false,
            success: function (response) {
                var cartItems = [];
                cartItems = JSON.parse(localStorage.getItem("userCarts"));
                if (cartItems != null) {
                    var isDuplicateItem = cartItems.filter(x => x.ProductTypeID == productId);
                    if (isDuplicateItem.length > 0)
                        toastr.error("", "Product is alread Exist...!");
                    else {
                        cartItems.push(response);
                        localStorage.setItem("userCarts", JSON.stringify(cartItems));
                        toastr.success("", "Product added successfully !");
                    }
                }
                else {
                    var result = [];
                    result.push(response);
                    localStorage.setItem("userCarts", JSON.stringify(result));
                    toastr.success("", "Product added successfully !");
                }
                cartItems = JSON.parse(localStorage.getItem("userCarts"));
                if (cartItems != null)
                    $("#PRODUCTCOUNT").text(cartItems.length);
                else
                    $("#PRODUCTCOUNT").text(0);
            }
        });
        AddCardItemCount();
    }

    function BuyToCart(productId) {
        $.ajax({
            type: 'GET',
            url: '/Home/CartJsonRecord?productId=' + productId,
            contentType: 'application/json; charset=utf-8',
            dataType: 'json',
            async: false,
            success: function (response) {
                var cartItems = [];
                cartItems = JSON.parse(localStorage.getItem("userCarts"));
                if (cartItems != null) {
                    var isDuplicateItem = cartItems.filter(x => x.ProductTypeID == productId);
                    if (isDuplicateItem.length > 0)
                        window.location.href = "/Home/Cart";
                    else {
                        cartItems.push(response);
                        localStorage.setItem("userCarts", JSON.stringify(cartItems));
                        window.location.href = "/Home/Cart";
                        toastr.success("", "Product added successfully !");
                    }
                }
                else {
                    var result = [];
                    result.push(response);
                    localStorage.setItem("userCarts", JSON.stringify(result));
                    window.location.href = "/Home/Cart";
                    toastr.success("", "Product added successfully !");
                }
            }
        });
        AddCardItemCount();
    }

